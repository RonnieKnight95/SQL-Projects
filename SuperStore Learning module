-- Ordering prices from highest to lowest
Select *
From superstore
Order by price desc;
-- Avg price of all items
Select ROUND(avg(price),2)
From superstore;
-- Total price of all items in the Kitchen Supplies category
SELECT ROUND(SUM(price), 2) 
From superstore
Where category = 'Kitchen Supplies';
-- Show items with quantity over or equal to 50 and the rating is below 4.5.
Select *
From superstore
Where stock_quantity >= 50 and average_rating <= 4.5;
